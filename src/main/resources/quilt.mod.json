{
  "schema_version": 1,
  "quilt_loader": {
    "group": "gay.lemmaeof",
    "id": "oneboxatatime",
    "version": "${version}",

    "name": "One Box At A Time",
    "description": "Messing around with the One Block At A Time april fools snapshot!",
    "authors": [
      "LemmaEOF"
    ],
    "contact": {
      "homepage": "https://lemmaeof.gay/",
      "sources": "https://github.com/lemmaeof/mc-aprilfools-2022"
    },

    "license": "CC0-1.0",
    "icon": "assets/oneoxatatime/icon.png",

    "environment": "*",
    "entrypoints": {
      "init": [
        "gay.lemmaeof.obaat.OneBoxAtATime"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "java",
        "version": ">=17"
      }
    ]
  },
  "mixin": [
    "obaat.mixins.json"
  ],
  "access_widener": [
    "obaat.accesswidener"
  ]
}
